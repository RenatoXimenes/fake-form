{
  "manifest_version": 3,
  "name": "Form Faker",
  "version": "1.0",
  "description": "Automatically fills forms with fake data.",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
